{
    "name":"maple",
    "version": "0.0.1",
    "publisher": "余江涛",
    "description": "Maple support for Visual Studio Code",
    "engines": { "vscode": "*" },
    "categories": [
		"Languages",
		"Snippets"
	],
    "contributes": {
        "languages": [
            {
                "id": "maple",
                "extensions": [".mpl",".mph"],
                "configuration": "maple.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "maple",
                "scopeName": "source.maple",
                "path": "./syntaxes/maple.json"
	    	}
        ],
        "snippets": [
            {
                "language": "maple",
                "path": "./snippets/maple.json"
            }
        ]
    },
    "icon":"img/maple.png" 
}